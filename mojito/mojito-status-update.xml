<?xml version="1.0" encoding="utf-8"?>

<node name="/Status_Update_Iface" xmlns:tp="http://telepathy.freedesktop.org/wiki/DbusSpec#extensions-v0">
  <interface name="com.intel.Mojito.StatusUpdate">
    <method name="UpdateStatus" tp:name-for-bindings="Update_Status">
      <arg name="status_message" type="s" direction="in"/>
    </method>

    <signal name="StatusUpdated" tp:name-for-bindings="Status_Updated">
      <arg name="success" type="b"/>
    </signal>
  </interface>
</node>
